<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.groupout.users_and_groups.pojos.DeviceType" table="device_type">
		<id name="typeId" type="string" length="32" >
			<column name="type_id" />
		</id>
		<property name="type" type="string" length="16" >
			<column name="type" />
		</property>
	</class>
</hibernate-mapping>