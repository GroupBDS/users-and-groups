<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.groupout.users_and_groups.pojos.GroupType" table="group_type">
		<id name="groupTypeId" type="string" length="32" >
			<column name="group_type_id" />
		</id>
		<property name="type" type="string" length="16" >
			<column name="type" />
		</property>
		<property name="description" type="string" length="512" >
			<column name="description" />
		</property>
		</class>
</hibernate-mapping>